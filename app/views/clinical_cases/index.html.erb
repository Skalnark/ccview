<p id="notice"><%= notice %></p>

<h1>Clinical Cases</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Auxtext</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clinical_cases.each do |clinical_case| %>
      <tr>
        <td><%= clinical_case.name %></td>
        <td><%= clinical_case.description %></td>
        <td><%= clinical_case.auxText %></td>
        <td><%= link_to 'Show', clinical_case %></td>
        <td><%= link_to 'Edit', edit_clinical_case_path(clinical_case) %></td>
        <td><%= link_to 'Destroy', clinical_case, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clinical Case', new_clinical_case_path %>
